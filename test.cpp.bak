#include <fstream>
#include <iostream>
int main() {
  std::ifstream fbe("raktar.txt");
  std::string s;
  int pr = 0, in = 0;
  for (int i = 0; i < 3; i++) {
    getline(fbe, s);
    pr = fbe.get();
    fbe >> in;
    fbe.get();
    std::cout << s << " " << pr << " " << in << "\n";
  }

  fbe.close();
  return 0;
}
